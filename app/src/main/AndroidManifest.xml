<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sachin.example.quotify">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/quotify_logo_rectangle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/quote_logo_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuotesAppUsingViewholder">
        <activity
            android:name="sachin.example.quotify.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>